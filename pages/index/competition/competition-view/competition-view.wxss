/* pages/index/competition/competition-view/competition-view.wxss */
page{
  background-color: white;
}

.swiper-tab{
  text-align: center;
  height: 90rpx;
  line-height: 90rpx;
  display: flex;
  flex-flow: row;
  justify-content: space-between;
  color:#00000049;
  background-color: #FFFFFF;
  border-bottom: 1px solid #e9e9e9;
}

.swiper-tab-item{
  width: 25%; 
  font-size: 32rpx;
  font-weight: bold;
  position: relative;
}

.swiper-tab-item:first-child{  
  margin-left: 20%;
}

.swiper-tab-item:nth-child(2){
  margin-right: 20%;
}

.active{
  color:#69c0f3
}

.identity-info{
  position: relative;
  margin-top: 50rpx;
  width: 100%;
  height: 70rpx;
}

.show-name{
  position: relative;
  float: left;
  width: 33%;
  height: 70rpx;
  line-height: 70rpx;
  font-size: 36rpx;
  font-weight: bold;
  text-align: center;
}

.show-phone{
  position: relative;
  float: left;
  margin-top: 10rpx;
  width: 84%;
  height: 70rpx;
  margin-left: 8%;
  font-size: 36rpx;
  font-weight: bold;
}

