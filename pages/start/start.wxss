/* pages/start/start.wxss */
image{
  width: 100%;
  height: 90%;
  vertical-align: middle;
}
button{
  position: fixed;
  bottom: 130rpx;
  left: 50%;
  transform: translateX(-50%);
  width: 280rpx;
  height: 80rpx;
  line-height: 80rpx;
  border: 2rpx solid #cfd0d1;
  border-radius: 15rpx;
  font-size: 28rpx;
  letter-spacing: 4rpx;
  color: rgb(0, 0, 0);
}

.start-view{
  width:100%;
  height: 100%;
}
.start-image{
  width:100%;
  height: 90%;
  text-align: center;
  margin-left: 0rpx;
  margin-top: 0rpx;
  opacity: 0.8;
  filter: alpha(opacity=50); 
}

page {
  background:rgb(184,196,214);
} 