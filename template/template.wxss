 .tabBar{
  width:100%;
  height: 80rpx;
  position: fixed;
  bottom:0;
  background:#ffffff;
  font-size:22rpx;
  color:#999999;
  padding: 10rpx 0;
  letter-spacing: 2rpx;
  z-index: 999;
  border-top: 2rpx solid #e9e9e9;
}
.tabBar-item{
  float:left;
  /* width:25%; */
  width:33%;
  text-align: center;
  overflow: hidden;
  /* border-width: 2rpx 2rpx 2rpx 2rpx ;
  border-color: rgb(63, 221, 70);
  border-style: solid; */
}
.tabBar-item:first-child .icon{
  width: 48rpx;
  height: 48rpx;   
}
.tabBar-item:nth-child(2) .icon{
  width: 48rpx;
  height: 48rpx;
}
.tabBar-item:nth-child(3) .icon{
  width: 48rpx;
  height: 48rpx;
}
